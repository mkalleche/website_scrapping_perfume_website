{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "functions": {
    "app/api/admin/scraping/**/*.ts": {
      "maxDuration": 300,
      "memory": 2048
    },
    "app/api/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
  },
  "crons": [
    {
      "path": "/api/admin/scraping/cron/nightly",
      "schedule": "0 2 * * *"
    }
  ]
}
